<action>
	<icon>inode-symlink</icon>
	<name>Paste as Link</name>
	<submenu></submenu>
	<unique-id>1728106758625839-1</unique-id>
	<command>wl-paste | xargs -I {} -d &quot;\n&quot; ln {} -S $(pwd)</command>
	<description>Paste currently selected items as Symbolic Links in current directory</description>
	<range>1+</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
